{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "extends": [
    "prettier",
    "plugin:sonarjs/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "arrow-parens": "off",
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "plugins": [ "sonarjs"]
}
